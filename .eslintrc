{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "array-bracket-spacing": [2, "always"],
    "comma-dangle": [2, "never"],
    "consistent-return": 0,
    "curly": 0,
    "eqeqeq": [2, "smart"],
    "max-len": 0,
    "no-param-reassign": [2, { "props": false }],
    "no-shadow": 0,
    "prefer-template": 0,
    "semi": [2, "never"]
  }
}
